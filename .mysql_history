CREATE TABLE linnad(
id INT NOT NULL AUTO_INCREMENT PRIMARY KEY,
nimi VARCHAR(50),
pealinn INT DEFAULT 0,
    riik VARCHAR(50),
    kontinent VARCHAR(50),
    elanikearv INT
);
CREATE TABLE linnad( id INT NOT NULL AUTO_INCREMENT PRIMARY KEY, nimi VARCHAR(50), pealinn INT DEFAULT 0,     riik VARCHAR(50),     kontinent VARCHAR(50),     elanikearv INT );
