drop table yo;
drop table users;
CREATE TABLE users(
id INT NOT NULL AUTO_INCREMENT PRIMARY KEY,
name VARCHAR(255) NOT NULL,
email VARCHAR(255) NOT NULL,
phone VARCHAR(255) NOT NULL,
created TIMESTAMP DEFAULT CURRENT_TIMESTAMP, 
last_login DATETIME DEFAULT NULL,
password VARCHAR(128),
UNIQUE(phone),
UNIQUE(email)
);
CREATE TABLE users( id INT NOT NULL AUTO_INCREMENT PRIMARY KEY, name VARCHAR(255) NOT NULL, email VARCHAR(255) NOT NULL, phone VARCHAR(255) NOT NULL, created TIMESTAMP DEFAULT CURRENT_TIMESTAMP,  last_login DATETIME DEFAULT NULL, password VARCHAR(128), UNIQUE(phone), UNIQUE(email) );
CREATE TABLE yo(
   id INT NOT NULL AUTO_INCREMENT PRIMARY KEY,
   sender INT NOT NULL,
   recipient INT NOT NULL,
   sent TIMESTAMP DEFAULT CURRENT_TIMESTAMP,
   recieved DATETIME DEFAULT NULL,
   FOREIGN KEY(recipient) REFERENCES users(id),
   FOREIGN KEY(sender) REFERENCES users(id)
);
CREATE TABLE yo(    id INT NOT NULL AUTO_INCREMENT PRIMARY KEY,    sender INT NOT NULL,    recipient INT NOT NULL,    sent TIMESTAMP DEFAULT CURRENT_TIMESTAMP,    recieved DATETIME DEFAULT NULL,    FOREIGN KEY(recipient) REFERENCES users(id),    FOREIGN KEY(sender) REFERENCES users(id) );
select * from users;
