SELECT * FROM raamatud WHERE antud='1';
SELECT * FROM raamatud GROUP BY raamatukogu;
select count(*) from raamatud;
select count(raamatukogu) from raamatud;
select distinct raamatukogu from raamatud;
select count(distinct raamatukogu) from raamatud;
select distinct id, raamatukogu from raamatud;
select distinct raamatunimi, raamatukogu from raamatud;
select count(distinct raamatunimi) from raamatud;
select count(distinct raamatukogu) from raamatud;
select count(distinct raamatunimi, raamatukogu) from raamatud;
select raamatukogu, count(distinct raamatunimi) from raamatud;
select raamatukogu, count(raamatunimi) from raamatud;
select count(*), raamatukogu from raamatud;
select from raamatud group by raamatukogu;
select from raamatud group by epost;
select *  from raamatud group by epost;
select epost from raamatud group by raamatukogu;
select epost, raamatukogu from raamatud group by raamatud;
select epost, raamatukogu from raamatud group by raamatukogu;
UPDATE raamatud SET raamatukogu='Alamraamatukogu' WHERE autor='Aun Auto';
select epost, raamatukogu from raamatud group by raamatukogu;
UPDATE raamatud SET rkirje='1' where raamatukogu='Alamraamatukogu';
select count(*) from raamatud group by raamatukogu;
select * from raamatud;
select count(raamatunimi) group by raamatukogu;
select count(raamatunimi) from raamatud group by raamatukogu;
select raamatukogu, count(raamatunimi) from raamatud group by raamatukogu;
select rkirje, raamatukogu, count(raamatunimi) from raamatud group by raamatukogu;
select raamatukogu, count(raamatunimi) from raamatud group by raamatukogu;
select * from raamatud;
select epost, raamatukogu from raamatud;
select raamatukogu, rkirje, count(raamatunimi) from raamatud;
select raamatukogu, countr(raamatunimi) from raamatud group by raamatukogu;
select * from raamatud;
select * from raamatud group by rkirje;
select * from raamatud distinct(raamatunimi) group by rkirje;
select raamatukogu from raamatud distinct(raamatunimi) group by rkirje;
select raamatukogu from raamatud distinct(raamatunimi) group by epost;
select epost, raamatukogu, rkirje from raamatud;
select epost, raamatukogu, rkirje, antud from raamatud;
select epost, distinct(raamatukogu), rkirje, antud from raamatud;
select epost, raamatukogu, rkirje, antud from raamatud;
select distinct raamatukogu;
select distinct raamatukogu from raamatud;
select rkirje, antud, saabunud distinct raamatukogu from raamatud;
select cound(raamatunimi) distinct raamatukogu from raamatud;
select count(raamatunimi)from raamatud group by raamatukogu;
select raamatukogu, count(raamatunimi)from raamatud group by raamatukogu;
select raamatukogu, count(raamatunimid), rkirje, antud from raamatud group by raamatukogu;
select raamatukogu, count(raamatunimi), rkirje, antud from raamatud group by raamatukogu;
select * from notes;
select * from tellimus;
show tables;
CREATE TABLE riigid(
id INT NOT NULL PRIMARY KEY,
nimi VARCHAR(12),
pealinn VARCHAR(12),
pealinnaelanikud INT,
üldelanikud
);
CREATE TABLE riigid( id INT NOT NULL PRIMARY KEY, nimi VARCHAR(12), pealinn VARCHAR(12), pealinnaelanikud INT, üldelanikud );
CREATE TABLE riigid(
id INT NOT NULL PRIMARY KEY,
nimi VARCHAR(12),
pealinn VARCHAR(12),
pealinnaelanikud INT,
üldelanikud,
);
CREATE TABLE riigid( id INT NOT NULL PRIMARY KEY, nimi VARCHAR(12), pealinn VARCHAR(12), pealinnaelanikud INT, üldelanikud, );
CREATE TABLE riigid(
id INT NOT NULL PRIMARY KEY,
nimi VARCHAR(12),
pealinn VARCHAR(12),
pealinnaelanikud INT,
üldelanikud INT
);
CREATE TABLE riigid( id INT NOT NULL PRIMARY KEY, nimi VARCHAR(12), pealinn VARCHAR(12), pealinnaelanikud INT, üldelanikud INT );
select * from riigid;
