drop table autoarvelevõtt;
CREATE TABLE autoarvelevõtt(
id VARCHAR NOT NULL PRIMARY KEY,
arvelevõtuaeg DATETIME,
arvelevõtuajakommentaar TEXT,
kliendinimi VARCHAR(30),
kontakt VARCHAR(30),
aktsepteeritud INT DEFAULT 0,
remondid TEXT,
edasimüük DATETIME,
edasimüügikommentaar TEXT,
edasimüük2 DATETIME,
edasimüügi2kommentaar TEXT,
mahakantud INT DEFAULT 0
);
CREATE TABLE autoarvelevõtt( id VARCHAR NOT NULL PRIMARY KEY, arvelevõtuaeg DATETIME, arvelevõtuajakommentaar TEXT, kliendinimi VARCHAR(30), kontakt VARCHAR(30), aktsepteeritud INT DEFAULT 0, remondid TEXT, edasimüük DATETIME, edasimüügikommentaar TEXT, edasimüük2 DATETIME, edasimüügi2kommentaar TEXT, mahakantud INT DEFAULT 0 );
CREATE TABLE autoarvelevõtt(
id VARCHAR(30) NOT NULL PRIMARY KEY,
arvelevõtuaeg DATETIME,
arvelevõtuajakommentaar TEXT,
kliendinimi VARCHAR(30),
kontakt VARCHAR(30),
aktsepteeritud INT DEFAULT 0,
remondid TEXT,
edasimüük DATETIME,
edasimüügikommentaar TEXT,
edasimüük2 DATETIME,
edasimüügi2kommentaar TEXT,
mahakantud INT DEFAULT 0
);
CREATE TABLE autoarvelevõtt( id VARCHAR(30) NOT NULL PRIMARY KEY, arvelevõtuaeg DATETIME, arvelevõtuajakommentaar TEXT, kliendinimi VARCHAR(30), kontakt VARCHAR(30), aktsepteeritud INT DEFAULT 0, remondid TEXT, edasimüük DATETIME, edasimüügikommentaar TEXT, edasimüük2 DATETIME, edasimüügi2kommentaar TEXT, mahakantud INT DEFAULT 0 );
INSERT INTO autoarvelevõtt(remondid) VALUES ('Auto on korras') WHERE id='815MBG';
