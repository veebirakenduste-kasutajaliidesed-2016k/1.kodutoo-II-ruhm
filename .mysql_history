CREATE TABLE tellimus(
  id INT NOT NULL AUTO_INCREMENT PRIMARY KEY,
  toode VARCHAR(30),
  aeg DATETIME UNIQUE,
  kliendinimi VARCHAR(30),
  kontakt VARCHAR(30),
  aktsepteeritud INT DEFAULT 0,
  summa DECIMAL(8, 2),
  arvetasutud INT DEFAULT 0,
  saadetud INT DEFAULT 0,
  kohal INT DEFAULT 0,
  tagasiside TEXT,
  lopetatud INT DEFAULT 0
);
CREATE TABLE tellimus(   id INT NOT NULL AUTO_INCREMENT PRIMARY KEY,   toode VARCHAR(30),   aeg DATETIME UNIQUE,   kliendinimi VARCHAR(30),   kontakt VARCHAR(30),   aktsepteeritud INT DEFAULT 0,   summa DECIMAL(8, 2),   arvetasutud INT DEFAULT 0,   saadetud INT DEFAULT 0,   kohal INT DEFAULT 0,   tagasiside TEXT,   lopetatud INT DEFAULT 0 );
