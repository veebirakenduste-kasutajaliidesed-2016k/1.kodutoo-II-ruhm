CREATE TABLE raamatud(
  id INT NOT NULL AUTO_INCREMENT PRIMARY KEY,
  raamatunimi VARCHAR(30),
  autor VARCHAR(30),
  aasta DATE,
  epost VARCHAR(30),
  atagasiside TEXT,
  kindlalerkogule INT DEFAULT 0,
  raamatukogu VARCHAR(30),
  rkirje INT DEFAULT 0,
  antud INT DEFAULT 0,
  saabunud INT DEFAULT 0,
  rtagasiside TEXT,
);
CREATE TABLE raamatud(   id INT NOT NULL AUTO_INCREMENT PRIMARY KEY,   raamatunimi VARCHAR(30),   autor VARCHAR(30),   aasta DATE,   epost VARCHAR(30),   atagasiside TEXT,   kindlalerkogule INT DEFAULT 0,   raamatukogu VARCHAR(30),   rkirje INT DEFAULT 0,   antud INT DEFAULT 0,   saabunud INT DEFAULT 0,   rtagasiside TEXT, );
CREATE TABLE raamatud(
  id INT NOT NULL AUTO_INCREMENT PRIMARY KEY,
  raamatunimi VARCHAR(30),
  autor VARCHAR(30),
  aasta DATE,
  epost VARCHAR(30),
  atagasiside TEXT,
  kindlalerkogule INT DEFAULT 0,
  raamatukogu VARCHAR(30),
  rkirje INT DEFAULT 0,
  antud INT DEFAULT 0,
  saabunud INT DEFAULT 0,
  rtagasiside TEXT
);
