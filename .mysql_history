select mudel, aasta, (select min(aasta) from autod as tabel2 where tabel1.mudel = tabel2.mudel) from autod as tabel1;
select mudel, aasta, (select min(aasta) from autod as tabel2 where tabel1.mudel = tabel2.mudel) as vanim from autod as tabel1;
select mudel, aasta, (select min(aasta) from autod as tabel2 where tabel1.mudel = tabel2.mudel) as vanim from autod as tabel1 group by mudel;
