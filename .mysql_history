CREATE TABLE tellimus(
id INT NOT NOULL AUTO_INCREMENT PRIMARY KEY,
aeg DATETIME,
kliendinimi VARCHAR(30),
kontakt VARCHAR(30),
aktsepteeritud INT DEFAULT 0,
arvetasutud INT DEFAULT 0,
saadetud INT DEFAULT 0,
kohal INT DEFAULT 0,
tagasiside TEXT,
lopetatud INT DEFAULT 0,
);
