UPDATE raamatud SET atagasiside='Natuke katki' WHERE raamatunimi='Kõik autodest';
UPDATE raamatud SET atagasiside='Korralik' WHERE raamatunimi='Sipsik';
UPDATE raamatud SET atagasiside='Täiesti puruks' WHERE raamatunimi='Raamat1';
UPDATE raamatud SET atagasiside='Täiesti terve, uhiuus' WHEER raamatunimi='Eragon';
UPDATE raamatud SET atagasiside='Täiesti terve, uhiuus' WHERE raamatunimi='Eragon';
SELECT * FROM raamatud;
