CREATE TABLE autoremont(
  id INT NOT NULL AUTO_INCREMENT PRIMARY KEY,
  aeg DATETIME,
  kliendinimi VARCHAR(30),
  kontakt VARCHAR(30),
  aktsepteeritud INT DEFAULT 0,
  mehhaanikusumma DECIMAL(8, 2),
  mehhaanikukommentaar TEXT,
  elektrikusumma DECIMAL(8, 2),
  elektrikukommentaar TEXT,
  valmisteatatud INT DEFAULT 0,
  arvetasutud INT DEFAULT 0,
  lopetatud INT DEFAULT 0
);
