drop table Tellimus;
CREATE TABLE Tellimus(
  ID INT NOT NULL AUTO_INCREMENT PRIMARY KEY,
  Toode VARCHAR(30),
  Eesnimi VARCHAR(30),
  Perekonnanimi VARCHAR(30),
  Aadress VARCHAR(30),
  Skuupaev DATETIME UNIQUE,
  Email VARCHAR(30),
  Aktsepteeritud VARCHAR(30) DEFAULT "Ei",
  summa DECIMAL(8, 2),
  arvetasutud INT DEFAULT 0,
  saadetud INT DEFAULT 0,
  kohal INT DEFAULT 0,
  tagasiside TEXT,
  lopetatud INT DEFAULT 0
);
CREATE TABLE Tellimus(   ID INT NOT NULL AUTO_INCREMENT PRIMARY KEY,   Toode VARCHAR(30),   Eesnimi VARCHAR(30),   Perekonnanimi VARCHAR(30),   Aadress VARCHAR(30),   Skuupaev DATETIME UNIQUE,   Email VARCHAR(30),   Aktsepteeritud VARCHAR(30) DEFAULT "Ei",   summa DECIMAL(8, 2),   arvetasutud INT DEFAULT 0,   saadetud INT DEFAULT 0,   kohal INT DEFAULT 0,   tagasiside TEXT,   lopetatud INT DEFAULT 0 );
