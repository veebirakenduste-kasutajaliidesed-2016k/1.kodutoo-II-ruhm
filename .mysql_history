   `idToode` VARCHAR(45) NOT NULL,
   `idHind` INT NOT NULL,
   `idKirjeldus` VARCHAR(45) NOT NULL,
   `idAlamkategooria` VARCHAR(45) NOT NULL,
   KEY ix_table1_field1 (Toode),
   PRIMARY KEY (`idToode`) )
 ENGINE = InnoDB;
